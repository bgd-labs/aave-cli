{
  "name": "@bgd-labs/aave-cli",
  "version": "0.6.4",
  "description": "A cli to perform various aave governance related tasks",
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx src/cli.ts",
    "build": "tsup",
    "test": "vitest",
    "ci:publish": "npm run build && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgd-labs/report-engine.git"
  },
  "author": "BGD labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bgd-labs/report-engine/issues"
  },
  "homepage": "https://github.com/bgd-labs/report-engine#readme",
  "devDependencies": {
    "@types/object-hash": "^3.0.6",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "bin": {
    "aave-cli": "dist/cli.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bgd-labs/aave-address-book": "^2.18.1",
    "@bgd-labs/js-utils": "^1.1.1",
    "@commander-js/extra-typings": "^11.1.0",
    "@inquirer/prompts": "^3.3.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.1",
    "find-object-paths": "^1.0.2",
    "gray-matter": "^4.0.3",
    "ipfs-only-hash": "^4.0.0",
    "json-bigint": "^1.0.0",
    "object-hash": "^3.0.0",
    "viem": "^2.6.1",
    "zod": "^3.22.4"
  }
}
